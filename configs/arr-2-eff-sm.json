{
    "name": "arr-2-eff-sm",
    "project": "hk-rhythm-2",
    "job_dir": "./results/arr-2-eff-sm",
    "datasets": [{
        "name": "icentia11k",
        "path": "./datasets/icentia11k",
        "params": {}
    }, {
        "name": "ptbxl",
        "path": "./datasets/ptbxl",
        "params": {}
    }, {
        "name": "lsad",
        "path": "./datasets/lsad",
        "params": {}
    }],
    "num_classes": 2,
    "class_map": {
        "0": 0,
        "7": 1,
        "8": 1
    },
    "class_names": [
        "NONE", "AFIB/AFL"
    ],
    "sampling_rate": 100,
    "frame_size": 512,
    "model_file": "model.keras",
    "samples_per_patient": [10, 10],
    "val_samples_per_patient": [10, 10],
    "val_file": "./results/${task}-class-2-${dataset}-${sampling_rate}fs-${frame_size}sz.pkl",
    "test_file": "./results/${task}-class-2-${dataset}-${sampling_rate}fs-${frame_size}sz.pkl",
    "val_patients": 0.20,
    "test_samples_per_patient": [10, 10],
    "test_size": 40000,
    "batch_size": 256,
    "buffer_size": 100000,
    "epochs": 150,
    "steps_per_epoch": 50,
    "val_metric": "loss",
    "lr_rate": 1e-3,
    "lr_cycles": 1,
    "class_weights": "balanced",
    "threshold": 0.75,
    "val_acc_threshold": 0.98,
    "tflm_var_name": "g_rhythm_model",
    "tflm_file": "rhythm_model_buffer.h",
    "backend": "pc",
    "demo_size": 800,
    "display_report": true,
    "quantization": {
        "enabled": false,
        "qat": false,
        "ptq": false,
        "input_type": "int8",
        "output_type": "int8",
        "supported_ops": null
    },
    "preprocesses": [
        {
            "name": "filter",
            "params": {
                "lowcut": 1.0,
                "highcut": 30,
                "order": 3,
                "forward_backward": true,
                "axis": 0
            }
        },
        {
            "name": "znorm",
            "params": {
                "eps": 0.01,
                "axis": null
            }
        }
    ],
    "architecture": {
        "name": "efficientnetv2",
        "params": {
            "input_filters": 16,
            "input_kernel_size": [1, 9],
            "input_strides": [1, 2],
            "blocks": [
                {"filters": 24, "depth": 2, "kernel_size": [1, 9], "strides": [1, 2], "ex_ratio": 1,  "se_ratio": 2},
                {"filters": 32, "depth": 2, "kernel_size": [1, 9], "strides": [1, 2], "ex_ratio": 1,  "se_ratio": 2},
                {"filters": 40, "depth": 2, "kernel_size": [1, 9], "strides": [1, 2], "ex_ratio": 1,  "se_ratio": 2},
                {"filters": 48, "depth": 1, "kernel_size": [1, 9], "strides": [1, 2], "ex_ratio": 1,  "se_ratio": 2}
            ],
            "output_filters": 0,
            "include_top": true,
            "use_logits": true
        }
    }
}
