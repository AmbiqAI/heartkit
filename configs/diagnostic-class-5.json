{
    "name": "diagnostic-class-5-ptbxl",
    "job_dir": "./results/diagnostic-class-5-lsad",
    "ds_path": "./datasets",
    "datasets": [{
        "name": "lsad",
        "params": {}
    }],
    "num_classes": 5,
    "class_map": {
        "0": 0,
        "1": 1,
        "2": 2,
        "3": 3,
        "4": 4
    },
    "class_names": [
        "NORM", "STTC", "MI", "HYP", "CD"
    ],
    "sampling_rate": 100,
    "frame_size": 800,
    "model_file": "model.keras",
    "samples_per_patient": [5, 5, 25, 5, 5],
    "val_file": "./results/${task}-class-5-${dataset}-${sampling_rate}fs-${frame_size}sz.pkl",
    "test_file": "./results/${task}-class-5-${dataset}-${sampling_rate}fs-${frame_size}sz.pkl",
    "val_samples_per_patient": [5, 5, 25, 5, 5],
    "val_patients": 0.20,
    "val_size": 40000,
    "threshold": 0.5,
    "test_samples_per_patient": [5, 5, 25, 5, 5],
    "test_size": 40000,
    "batch_size": 256,
    "buffer_size": 100000,
    "epochs": 125,
    "steps_per_epoch": 20,
    "val_metric": "loss",
    "lr_rate": 1e-3,
    "lr_cycles": 1,
    "class_weights": "balanced",
    "label_smoothing": 0.1,
    "val_acc_threshold": 0.98,
    "tflm_var_name": "g_diagnostic_model",
    "tflm_file": "diagnostic_model_buffer.h",
    "backend": "pc",
    "quantization": {
        "enabled": true,
        "qat": false,
        "ptq": true,
        "input_type": "int8",
        "output_type": "int8",
        "supported_ops": null
    },
    "preprocesses": [
        {
            "name": "filter",
            "params": {
                "lowcut": 1.0,
                "highcut": 30,
                "order": 3,
                "forward_backward": true,
                "axis": 0
            }
        },
        {
            "name": "znorm",
            "params": {
                "eps": 0.01,
                "axis": null
            }
        }
    ],
    "architecture": {
        "name": "efficientnetv2",
        "params": {
            "input_filters": 32,
            "input_kernel_size": [1, 9],
            "input_strides": [1, 2],
            "blocks": [
                {"filters": 48,  "depth": 2, "kernel_size": [1, 9], "strides": [1, 2], "ex_ratio": 1, "se_ratio": 2},
                {"filters": 64,  "depth": 2, "kernel_size": [1, 9], "strides": [1, 2], "ex_ratio": 1, "se_ratio": 4},
                {"filters": 128, "depth": 2, "kernel_size": [1, 9], "strides": [1, 2], "ex_ratio": 1, "se_ratio": 4},
                {"filters": 192, "depth": 1, "kernel_size": [1, 9], "strides": [1, 2], "ex_ratio": 1, "se_ratio": 4}
            ],
            "output_filters": 0,
            "output_activation": "sigmoid",
            "include_top": true,
            "use_logits": true
        }
    }
}
