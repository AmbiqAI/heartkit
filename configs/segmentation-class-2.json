{
    "name": "segmentation-class-2",
    "job_dir": "./results/segmentation-class-2",
    "ds_path": "./datasets",
    "datasets": [{
        "name": "icentia11k",
        "params": {}
    }],
    "num_classes": 2,
    "class_map": {
        "0": 0,
        "1": 0,
        "2": 1,
        "3": 0,
        "4": 0
    },
    "class_names": [
        "NONE", "QRS"
    ],
    "sampling_rate": 100,
    "frame_size": 256,
    "model_file": "model.keras",
    "samples_per_patient": 10,
    "train_patients": 10000,
    "val_file": "segmentation-class-2-10000pt-100fs-256.pkl",
    "val_samples_per_patient": 20,
    "val_patients": 0.20,
    "val_size": 40000,
    "test_samples_per_patient": 10,
    "test_patients": 1000,
    "test_size": 10000,
    "batch_size": 256,
    "buffer_size": 40000,
    "epochs": 125,
    "steps_per_epoch": 50,
    "val_metric": "loss",
    "lr_rate": 1e-3,
    "lr_cycles": 1,
    "val_acc_threshold": 0.98,
    "tflm_var_name": "g_segmentation_model",
    "tflm_file": "segmentation_model_buffer.h",
    "backend": "pc",
    "quantization": {
        "enabled": true,
        "qat": false,
        "ptq": true,
        "input_type": "int8",
        "output_type": "int8",
        "supported_ops": null
    },
    "architecture": {
        "name": "unet",
        "params": {
            "blocks": [
                {"filters": 8, "depth": 1, "ddepth": 1, "kernel": [1, 3], "pool": [1, 3], "strides": [1, 2], "skip": true},
                {"filters": 12, "depth": 1, "ddepth": 1, "kernel": [1, 3], "pool": [1, 3], "strides": [1, 2], "skip": true},
                {"filters": 16, "depth": 1, "ddepth": 1, "kernel": [1, 3], "pool": [1, 3], "strides": [1, 2], "skip": true},
                {"filters": 24, "depth": 1, "ddepth": 1, "kernel": [1, 3], "pool": [1, 3], "strides": [1, 2], "skip": true},
                {"filters": 32, "depth": 1, "ddepth": 1, "kernel": [1, 3], "pool": [1, 3], "strides": [1, 2], "skip": true}
            ],
            "output_kernel_size": [1, 3],
            "include_top": true,
            "use_logits": true
        }
    },
    "preprocesses": [
        {
            "name": "filter",
            "params": {
                "lowcut": 1.0,
                "highcut": 30,
                "order": 3,
                "forward_backward": true,
                "axis": 0
            }
        },
        {
            "name": "znorm",
            "params": {
                "eps": 0.01,
                "axis": null
            }
        }
    ]
}
